{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": ["password", "email", "username"],
  "properties": {
    "password": {
      "type": "string",
      "minLength": 8
    },
    "email": {
      "type": "string",
      "minLength": 5,
      "maxLength": 60,
      "format": "email"
    },
    "username": {
      "type": "string",
      "minLength": 2,
      "maxLength": 25
    }
  },
  "additionalProperties": false
}
